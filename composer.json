{
    "name": "manishtechniz/hello-world",
    "type": "library",
    "autoload": {
        "psr-4": {
            "TestPackage\\HelloWorld\\": "src/"
        }
    },
    "homepage": "https://github.com/manishtechniz/hello-world",
    "authors": [
        {
            "name": "manishtechniz",
            "email": "manishtechniz@gmail.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0"
    },
   
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },

    "support": {
        "issues": "https://github.com/manishtechniz/hello-world/issues",
        "source": "https://github.com/manishtechniz/hello-world"  
    }
}
